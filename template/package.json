{
  "name": "{{ name }}",
  "description": "{{ description }}",
  "version": "0.0.0",
  "author": "{{ author }}",
  "main": "index.js",
  "engines": {
    "node": "*"
  },
  "dependencies": {},
  "peerDependencies": {
    {{#ifstyle}}
    "vueify-insert-css": "^1.0.0",
    {{/ifstyle}}
    "vue": "^1.0.16"
  },
  "devDependencies": {
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-runtime": "^6.7.5",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.0.0",
    "babel-runtime": "^6.0.0",
    {{#if_eq script "coffee"}}
    "coffee-loader": "^0.7.2",
    "coffee-script": "^1.10.0",
    {{/if_eq}}
    {{#if_eq template "pug"}}
    "pug": "^2.0.0",
    {{/if_eq}}
    {{#lint}}
    "eslint": "^2.0.0",
    "eslint-friendly-formatter": "^1.2.2",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-html": "^1.3.0",
    {{#if_eq lintConfig "standard"}}
    "eslint-config-standard": "^5.1.0",
    "eslint-plugin-promise": "^1.0.8",
    "eslint-plugin-standard": "^1.3.2",
    {{/if_eq}}
    {{#if_eq lintConfig "airbnb"}}
    "eslint-config-airbnb": "^6.2.0",
    {{/if_eq}}
    {{/lint}}
    "gh-pages": "^0.11.0",
    {{#unit}}
    "karma": "^0.13.22",
    "karma-chrome-launcher": "^0.2.3",
    "karma-firefox-launcher": "^0.1.7",
    "karma-mocha": "^0.2.2",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.26",
    "karma-vue-component": "^0.1.0",
    "karma-coverage": "^0.5.5",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sinon-chai": "^1.2.0",
    "karma-webpack": "^1.7.0",
    "lolex": "^1.4.0",
    "mocha": "^2.4.5",
    "chai": "^3.5.0",
    "sinon": "^1.17.3",
    "sinon-chai": "^2.8.0",
    "inject-loader": "^2.0.1",
    "isparta-loader": "^2.0.0",
    "phantomjs-prebuilt": "^2.1.3",
    {{/unit}}
    "script-runner": "^0.1.4",
    "template-html-loader": "0.0.3",
    "vue": "^1.0.21",
    "vue-compiler": "^0.1.1",
    "vue-dev-server": "^0.2.10",
    "vue-hot-reload-api": "^1.3.2",
    "vue-html-loader": "^1.2.2",
    "vue-loader": "^8.2.3",
    {{#ifstyle}}
    "css-loader": "^0.23.1",
    "vue-style-loader": "^1.0.0",
    "vueify-insert-css": "^1.0.0",
    {{/ifstyle}}
    "webpack": "^1.13.0"
  },
  "keywords": [
    "component",
    "vue"
  ],
  "readmeFilename": "README.md",
  "scripts": {
    "build:vue": "NODE_ENV=production vue-compiler --out . src/*.vue",
    "build:webpack": "webpack --config build/webpack.config.js",
    "build": "run-npm build:*",
    "dev": "vue-dev-server",
    "watch": "karma start test/karma.conf.js --browsers Chrome --auto-watch --reporters spec",
    "test": "karma start test/karma.conf.js --browsers PhantomJS --single-run",
    "preversion": "npm test",
    "version": "npm run build && git add .",
    "postversion": "git push && git push --tags && npm publish",
    "ghpages": "vue-dev-server --static static/ && gh-pages -d static"
  }
}
